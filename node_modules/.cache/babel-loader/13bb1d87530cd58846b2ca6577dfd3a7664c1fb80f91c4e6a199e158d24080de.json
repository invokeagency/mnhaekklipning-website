ght: 0.7em;\n  flex-shrink: 0;\n  transition: color 0.3s ease;\n}\n.features-list-bullets li:hover::before {\n  color: var(--forest-green);\n}\n@media (max-width: 700px) {\n  .features-list-bullets {\n    grid-template-columns: 1fr;\n    padding: 0 1rem;\n    gap: 0.7rem;\n  }\n}\n.about {\n  background: var(--forest-green);\n  padding: 6rem 1.2rem;\n  color: white;\n  margin: 3rem 0;\n  min-height: 600px;\n}\n\n.about-container {\n  max-width: 1200px;\n  margin: 0 auto;\n  display: flex;\n  gap: 4rem;\n  position: relative;\n}\n\n.about-content {\n  flex: 1;\n  padding-right: 2rem;\n}\n\n.about h2 {\n  font-family: 'Poppins', 'Nunito', Arial, sans-serif;\n  font-size: 1.8rem;\n  margin-bottom: 1.5rem;\n  color: white;\n}\n\n.about p {\n  margin-bottom: 1.2rem;\n  font-size: 1.15rem;\n  line-height: 1.6;\n  text-shadow: 0 1px 4px rgba(0,0,0,0.1);\n}\n\n.about blockquote {\n  font-family: 'Nunito', Arial, sans-serif;\n  font-size: 1.3rem;\n  color: var(--lawn-green);\n  margin-top: 2rem;\n  font-style: italic;\n  font-weight: 700;\n}\n\n.process-steps {\n  flex: 1;\n  position: relative;\n  min-height: 520px;\n}\n\n.process-card {\n  position: absolute;\n  width: 85%;\n  background: rgba(255, 255, 255, 0.1);\n  backdrop-filter: blur(8px);\n  border: 1px solid rgba(255, 255, 255, 0.2);\n  padding: 1.5rem;\n  border-radius: 12px;\n  transition: all 0.3s ease;\n}\n\n.process-card:hover {\n  transform: translateY(-5px);\n  background: rgba(255, 255, 255, 0.15);\n  border-color: var(--lawn-green);\n  box-shadow: 0 10px 20px rgba(0,0,0,0.1);\n}\n\n.process-card h3 {\n  font-family: 'Nunito', Arial, sans-serif;\n  font-size: 1.2rem;\n  font-weight: 700;\n  margin-bottom: 0.5rem;\n  color: var(--lawn-green);\n}\n\n.process-card p {\n  font-size: 1rem;\n  line-height: 1.5;\n  margin: 0;\n}\n\n@media (max-width: 900px) {\n  .about-container {\n    flex-direction: column;\n    gap: 2rem;\n  }\n\n  .process-steps {\n    margin-top: 2rem;\n  }\n\n  .process-card {\n    position: 