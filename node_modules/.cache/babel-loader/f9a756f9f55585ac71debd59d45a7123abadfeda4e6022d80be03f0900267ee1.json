he[5] || (_cache[5] = $event => $data.calc.wasteRemoval = $event)\n  }, null, 512 /* NEED_PATCH */), [[_vModelCheckbox, $data.calc.wasteRemoval]])]), _createElementVNode(\"div\", _hoisted_8, [_createElementVNode(\"a\", {\n    href: \"#\",\n    onClick: _cache[6] || (_cache[6] = _withModifiers($event => $data.showGuide = true, [\"prevent\"]))\n  }, \"Hvordan måler jeg min hæk?\")]), _createElementVNode(\"div\", _hoisted_9, [_cache[17] || (_cache[17] = _createElementVNode(\"span\", null, \"Estimeret pris (inkl. moms):\", -1 /* HOISTED */)), _createElementVNode(\"span\", _hoisted_10, _toDisplayString($options.formattedPrice), 1 /* TEXT */)]), _cache[18] || (_cache[18] = _createElementVNode(\"button\", {\n    type: \"submit\",\n    class: \"cta-btn\"\n  }, \"Få tilbud nu\", -1 /* HOISTED */))], 32 /* NEED_HYDRATION */)])]), _createCommentVNode(\" End hero-row \")]), _createCommentVNode(\" End hero-content-container \"), _createCommentVNode(\" Scroll Indicator \"), _createElementVNode(\"div\", {\n    class: \"scroll-indicator\",\n    onClick: _cache[8] || (_cache[8] = $event => $options.scrollTo('services'))\n  }, [_createCommentVNode(\" Still points to non-existent element \"), _cache[21] || (_cache[21] = _createElementVNode(\"span\", {\n    class: \"scroll-text\"\n  }, \"Rul ned for at høre mere om os\", -1 /* HOISTED */)), _cache[22] || (_cache[22] = _createElementVNode(\"svg\", {\n    width: \"40\",\n    height: \"40\",\n    viewBox: \"0 0 24 24\",\n    fill: \"none\",\n    xmlns: \"http://www.w3.org/2000/svg\"\n  }, [_createElementVNode(\"path\", {\n    d: \"M7 10L12 15L17 10\",\n    stroke: \"currentColor\",\n    \"stroke-width\": \"2\",\n    \"stroke-linecap\": \"round\",\n    \"stroke-linejoin\": \"round\"\n  })], -1 /* HOISTED */))])], 4 /* STYLE */), _createCommentVNode(\" Guide Modal \"), $data.showGuide ? (_openBlock(), _createElementBlock(\"div\", {\n    key: 0,\n    class: \"modal-overlay\",\n    onClick: _cache[11] || (_cache[11] = _withModifiers($event =>